{
	"rust-analyzer.serverPath": "/usr/bin/rust-analyzer",
	"clangd.arguments": [
		"--background-index",
		"--compile-commands-dir=build",
		"--fallback-style=WebKit",
		"--header-insertion=never",
		"--pch-storage=memory",
		"-j=6"
	],
	"coc.preferences.formatOnSaveFiletypes": ["*"],
	"python.formatting.provider": "yapf"
}
