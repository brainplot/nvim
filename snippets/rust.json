{
	"println!": {
		"prefix": "pln",
		"body": "println!(\"$1\");"
	},
	"println! with format param": {
		"prefix": "pln,",
		"body": "println!(\"$1{$2}$3\", $4);"
	},
	"eprintln!": {
		"prefix": "epln",
		"body": "eprintln!(\"$1\");"
	},
	"eprintln! with format param": {
		"prefix": "epln,",
		"body": "eprintln!(\"$1{$2}$3\", $4);"
	},
	"Ok(..)": {
		"prefix": "ok",
		"body": "Ok(${1:()})"
	},
	"Some(..)": {
		"prefix": "some",
		"body": "Some($1)"
	},
	"Unsafe expression": {
		"prefix": "unse",
		"body": "unsafe { $1 }${2:;}"
	},
	"Unsafe block": {
		"prefix": "unsb",
		"body": "unsafe {\n\t$1\n}${2:;}"
	},
	"Test function": {
		"prefix": "testf",
		"body": "#[test]\nfn $1() {\n\t$0\n}"
	},
	"dbg!(..)": {
		"prefix": "dbg",
		"body": "dbg!(${1:$TM_SELECTED_TEXT})"
	}
}
